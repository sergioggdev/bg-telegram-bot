{
  "$schema": "https://json.schemastore.org/prettierrc",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "overrides": [
    { "files": "*.json","options": { "parser": "json" } },
    { "files": ".swcrc","options": { "parser": "json" } }
  ],
  "parser": "typescript",
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false
}
